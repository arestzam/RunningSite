
@{
    ViewBag.Title = "ShowLastOrder";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">

    <h2>Your Order</h2>

    <p>Order No.: @ViewBag.OrderNo</p>
    <p>Order Date: @ViewBag.OrderDate</p>
    <p>Race Id: @ViewBag.RaceId</p>
    <p>Race Name: @ViewBag.RaceName</p>
    <p>Start Group: @ViewBag.StartGroup</p>
    <p>Total Amount Paid: €@ViewBag.TotalAmount</p>
    <p>Payment Reference: @ViewBag.PayPalReference</p>
    <p>
        @Html.ActionLink("Go Back", "Index", "Account")
    </p>
</div>
